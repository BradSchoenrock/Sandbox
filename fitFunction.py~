
import os
import sys
import datetime
import gc
import math  

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

from pandas import DataFrame

from scipy import optimize

def timeSeriesFunc(x, alpha, beta, gamma, mu1, mu2, mu3, sig1, sig2, sig3, A, B, C):
    return 1/math.sqrt(2*math.pi) * ( alpha/sig1 * numpy.exp( (-1*(x-mu1)**2) / (2*sig1**2) ) + beta/sig2 * numpy.exp( (-1*(x-mu2)**2) / (2*sig2**2) ) + gamma/sig3 * numpy.exp( (-1*(x-mu3)**2) / (2*sig3**2) ) + ) * ( 1 + A*cos( x/30 + B*cos( x/30 ) ) ) + C



x=1
alpha=2
beta=3
gamma=4
mu1=5
mu2=6
mu3=7
sig1=8
sig2=9
sig3=10
A=11
B=12
C=13

ans=timeSeriesFunc(x, alpha, beta, gamma, mu1, mu2, mu3, sig1, sig2, sig3, A, B, C)

print (ans)


